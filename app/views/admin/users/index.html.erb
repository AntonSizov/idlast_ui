<table class="table table-hover">
  <tr>
    <th>Email</th>
    <th>Username</th>
	<th>Updated At</th>
	<th>Created At</th>
  </tr>

  <% @users.each do |user| %>
    <tr>
      <td><%= link_to user.email, admin_user_path(user) %></td>
      <td><%= user.name %></td>
	  <td><%= user.updated_at %></td>
	  <td><%= user.created_at %></td>
    </tr>
  <% end %>

</table>
  <%= will_paginate %>

<br />

