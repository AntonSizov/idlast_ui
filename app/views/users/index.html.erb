<h1>Listing users</h1>

  <%= will_paginate %>

<table class="table table-hover">
  <tr>
    <th>Email</th>
    <th>Username</th>
  </tr>

  <% @users.each do |user| %>
    <tr>
      <td><%= user.email %></td>
      <td><%= user.name %></td>
    </tr>
  <% end %>

</table>
  <%= will_paginate %>

<br />

<%= link_to 'New user', new_user_path %>
